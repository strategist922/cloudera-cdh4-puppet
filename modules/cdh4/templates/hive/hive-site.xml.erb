<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<configuration>

<%- settings = hive_defaults.merge(hive_settings) -%>
<%- settings.keys.sort.each do |name| -%>
<%= %Q[
  <property>
    <name>#{name}</name>
    <value>#{settings[name]}</value>
  </property>] %>
<%- end -%>

</configuration>
